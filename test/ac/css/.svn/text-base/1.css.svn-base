@import url(top.css); 
#main{width:1000px; margin:0 auto; margin-top:30px;background-color:#fff; min-height:500px;margin-bottom:50px;
		padding-bottom:30px;   }
.main-table{width:960px; margin:0  auto; font-size:15px; }
.main-table td, .main-table th {padding: 15px;border-bottom: 1px dashed #f2f2f2;}
.main-table th:first-child{padding-left:0px;font-size:18px; }
.main-table th { font-size:13px;color:#1aa3e7; 
				text-align: left;text-shadow: 0 1px 0 rgba(255,255,255,.5);border-bottom: 1px solid #ccc;}
.main-table th a{color:#1aa3e7; }
.main-table td{font-size:13px }				
.main-table div.table-newinfo{height:10px; width:10px; border-radius:10px;cursor:pointer; 
							background-color:#1aa3e7; margin-left:15px;}
.main-table tr td:first-child{width:400px;padding-left:30px; font-size:15px;  } 
.main-table tr td:last-child{width:200px; text-align:center; color:#b2b0b0;} 
.main-table tr img{margin-right:5px;position:relative; top:4px;cursor:pointer; }
.main-table th a{margin-left:20px;} 